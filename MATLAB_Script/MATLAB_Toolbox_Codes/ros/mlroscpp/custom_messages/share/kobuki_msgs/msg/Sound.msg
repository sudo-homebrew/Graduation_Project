# Sends a command for playing sounds.
# The available sound sequences:
# 0 - turn on
# 1 - turn off
# 2 - recharge start
# 3 - press button,
# 4 - error sound
# 5 - start cleaning
# 6 - cleaning end

uint8 ON            = 0
uint8 OFF           = 1
uint8 RECHARGE      = 2
uint8 BUTTON        = 3
uint8 ERROR         = 4
uint8 CLEANINGSTART = 5
uint8 CLEANINGEND   = 6

uint8 value